    "deploy-android": "ionic build --prod && npx cap copy && npx cap sync && npx cap open android && cd android && fastlane beta",
    "deploy-ios": "ionic build --prod && npx cap copy && npx cap sync && npx cap open ios && cd ios && cd app && fastlane beta",
    "deploy-both": "npm run deploy-android && npm run deploy-ios",
    "deploy-github": "ionic build --prod --base-href https://timohe.github.io/tichu_tournament_dashboard/ && npx angular-cli-ghpages --branch=gh-pages --dir=www",
    "be": "cd /Users/timo/Documents/repos/tichu_backend/ && tsc && node ./built/server.js",
    "lab": "ionic serve -l",
    "dev": "ionic serve",
    "prod": "ng serve --configuration production",
    "backend": "cd /Users/timo/Documents/repos/tichu_backend/ && npm run prod",
    "dev-android": "ionic cap run android -l --external",
    "dev-ios": "ionic capacitor run ios -livereload --external",
    "prod-ios": "ionic build --prod && npx cap copy && npx cap open ios",
    "prod-android": "ionic build --prod && npx cap copy && npx cap open android"
  },
